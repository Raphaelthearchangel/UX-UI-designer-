	var getUrlParameter = function getUrlParameter(sParam) {
		var sPageURL = window.location.search.substring(1),
			sURLVariables = sPageURL.split('&'),
			sParameterName,
			i;
	
		for (i = 0; i < sURLVariables.length; i++) {
			sParameterName = sURLVariables[i].split('=');
	
			if (sParameterName[0] === sParam) {
				return typeof sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
			}
		}
		return false;
	};
	
	function setCookie(key, value, expiry) {
		var expires = new Date();
		expires.setTime(expires.getTime() + (expiry * 24 * 60 * 60 * 1000));
		document.cookie = key + '=' + value + ';path=/' + ';expires=' + expires.toUTCString();
	}

	function getCookie(key) {
		var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
		return keyValue ? keyValue[2] : null;
	}

	function eraseCookie(key) {
		var keyValue = getCookie(key);
		setCookie(key, keyValue, '-1');
	}
	
	var utm_source_get = getUrlParameter('utm_source');
	var utm_source_cookie = getCookie('utm_source');
	if( utm_source_get != false ){
		var utm_source = utm_source_get;
		setCookie( 'utm_source', utm_source, '7' );
	}else if( utm_source_cookie != false ){
		var utm_source = utm_source_cookie;
	}
	
